                                            continue;}
                                        if (moveNine == 1){
                                            tictactoe[0][0] = " X ";}
                                        if (moveNine == 2){
                                            tictactoe[0][1] = " X ";}
                                        if (moveNine == 3){
                                            tictactoe[0][2] = " X ";}
                                        if (moveNine == 4){
                                            tictactoe[1][0] = " X ";}
                                        if (moveNine == 5){
                                            tictactoe[1][1] = " X ";}
                                        if (moveNine == 6){
                                            tictactoe[1][2] = " X ";}
                                        if (moveNine == 7){
                                            tictactoe[2][0] = " X ";}
