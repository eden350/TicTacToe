                                        tictactoe[0][1] = " O ";}
                                    if (moveEight == 3){
                                        tictactoe[0][2] = " O ";}
                                    if (moveEight == 4){
                                        tictactoe[1][0] = " O ";}
                                    if (moveEight == 5){
                                        tictactoe[1][1] = " O ";}
                                    if (moveEight == 6){
                                        tictactoe[1][2] = " O ";}
                                    if (moveEight == 7){
                                        tictactoe[2][0] = " O ";}
                                    if (moveEight == 8){
                                        tictactoe[2][1] = " O ";}
