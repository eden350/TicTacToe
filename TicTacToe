                    while (true) {
                        int moveFive = s.nextInt();
                        if (moveFive == moveOne || moveFive == moveTwo || moveFive == moveThree || moveFive == moveFour) {
                            System.out.println("THIS CELL IS TAKEN! CHOOSE A DIFFRACT NUMBER");
                            continue;}
                        if (moveFive == 1){
                            tictactoe[0][0] = " X ";}
